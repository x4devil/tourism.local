{% extends '::adminbase.html.twig' %}

{% block title %}
    Просмотр субподрядчика
{% endblock %}
{% block body -%}
    <div class="panel panel-default">
        <div class="panel-heading panel-header">
            <h3 class="panel-title">Просмотр субподрядчика</h3>
        </div>
        <div class="panel-body">
            <div class="input-group">
                <span class="input-group-addon" id="name-group">ФИО</span>
                <input type="text" disabled value="{{entity.fio}}" class="form-control" aria-describedby="input-group-addon"/>
            </div>
            <div class="input-group">
                <span class="input-group-addon" id="base-group">База отдыха</span>
                <input type="text" disabled value="{{entity.base}}" class="form-control" aria-describedby="input-group-addon"/>
            </div>
            <div class="panel-actions">
                <a class="btn btn-default" href="{{ path('sublegal') }}">Назад</a>
            </div>
        </div>
    </div>
{% endblock %}

